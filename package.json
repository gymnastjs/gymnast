{
  "name": "xn-reflex",
  "version": "0.0.0-semantically-released",
  "description": "Configurable grid and layout engine for React",
  "main": "index.js",
  "browser": "dist/reflex.min.js",
  "homepage": "https://obartra.github.io/reflex",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "src",
    "dist",
    "version.js",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/obartra/reflex"
  },
  "bugs": {
    "url": "https://github.com/obartra/reflex/issues"
  },
  "author": "Oscar Bartra",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-actions": "3.2.19",
    "@storybook/addon-knobs": "3.2.19",
    "@storybook/addon-notes": "3.2.19",
    "@storybook/addon-options": "3.2.19",
    "@storybook/addon-storyshots": "3.2.19",
    "@storybook/channels": "3.2.19",
    "@storybook/react": "3.2.19",
    "all-contributors-cli": "4.10.1",
    "babel-cli": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-jest": "22.0.4",
    "babel-plugin-flow-react-proptypes": "13.0.0",
    "babel-plugin-lodash": "3.3.2",
    "babel-plugin-macros": "2.0.0",
    "babel-plugin-module-resolver": "3.0.0",
    "babel-plugin-preval": "1.6.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "6.26.0",
    "bithound": "1.7.0",
    "bundlesize": "0.15.3",
    "clean-webpack-plugin": "0.1.17",
    "codeclimate-test-reporter": "0.5.0",
    "commitizen": "2.9.6",
    "condition-circle": "2.0.1",
    "cross-env": "5.1.3",
    "css-loader": "0.28.7",
    "cssnano": "3.10.0",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.0",
    "eslint": "4.13.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-flowtype": "2.40.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "extract-text-webpack-plugin": "3.0.2",
    "find-up": "2.1.0",
    "flow-bin": "0.61.0",
    "flow-copy-source": "1.2.1",
    "html-loader": "0.5.1",
    "husky": "0.14.3",
    "ignore-loader": "0.1.2",
    "jest": "21.2.1",
    "lint-staged": "6.0.0",
    "lodash": "4.17.4",
    "markdown-loader": "2.0.1",
    "nightwatch": "0.9.19",
    "npm-run-all": "4.1.2",
    "picturebook": "1.0.35",
    "polished": "1.9.0",
    "postcss-cli": "4.1.1",
    "postcss-cssnext": "3.0.2",
    "postcss-discard-comments": "2.0.4",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-for": "2.1.1",
    "postcss-import": "11.0.0",
    "postcss-loader": "2.0.9",
    "postcss-nesting": "4.2.1",
    "prettier": "1.9.2",
    "raw-loader": "0.5.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-element-to-jsx-string": "13.1.0",
    "react-test-renderer": "16.2.0",
    "regenerator-runtime": "0.11.1",
    "saucelabs": "1.4.0",
    "selenium-standalone": "6.12.0",
    "semantic-release": "11.0.2",
    "storybook-addon-jsx": "5.1.0",
    "style-loader": "0.19.1",
    "stylelint": "8.4.0",
    "webpack": "3.10.0",
    "webpack-bundle-analyzer": "2.9.1"
  },
  "lint-staged": {
    "*.{js,css}": [
      "format",
      "git add"
    ]
  },
  "bundlesize": [
    {
      "path": "./dist/reflex.min.js",
      "maxSize": "11.0 Kb"
    }
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/{,**/}*.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/env.js",
    "moduleNameMapper": {
      "^.+[.]css$": "identity-obj-proxy",
      "^.+[.](md|txt)$": "<rootDir>/test/stringStub.js"
    },
    "transform": {
      "^.+[.]js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules\\/(?!picturebook)"
    ]
  },
  "scripts": {
    "build:dev": "webpack --config scripts/webpack.config.js",
    "build:min": "cross-env NODE_ENV=production webpack --config scripts/webpack.config.js -p",
    "build:types": "sh scripts/flowsource.sh",
    "build": "npm-run-all --parallel build:*",
    "clean:image": "find storybook/stories -name \"*.png\" -type f -delete",
    "commit": "git-cz",
    "contributors:add": "all-contributors add",
    "contributors:gen": "all-contributors generate",
    "format-all": "git ls-files {src,test,storybook}*.{js,css} | xargs yarn format",
    "format": "prettier --write --single-quote --no-semi --trailing-comma es5",
    "lint:css": "stylelint src/*.css stories/*.css",
    "lint:flow": "flow stop && flow --show-all-errors --include-warnings",
    "lint:js": "eslint scripts src stories storybook test",
    "lint": "npm-run-all --parallel lint:*",
    "precommit": "lint-staged",
    "start:storybook": "picturebook -p 9001 -s ./static -c node_modules/picturebook/config",
    "start:webpack": "yarn build:dev -- --watch",
    "start": "npm-run-all --parallel start:*",
    "test:bithound": "bithound check git@github.com:obartra/reflex.git",
    "test:image": "picturebook-image --env iphone7,ie11,safari,firefox,edge,chrome --suiteRetries 3",
    "test:size": "bundlesize",
    "test": "jest",
    "test:cover": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "release": {
    "verifyConditions": "condition-circle"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "prop-types": ">=15",
    "react": ">=15"
  },
  "dependencies": {
    "cxs": "https://github.com/arahansen/cxs#d593d14e708c4f5c17cb2105cb9435ae87ade362",
    "preval.macro": "1.0.2"
  }
}
