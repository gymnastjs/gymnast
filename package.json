{
  "name": "gymnast",
  "version": "0.0.0-semantically-released",
  "description": "Configurable grid and layout engine for React",
  "main": "index.js",
  "unpkg": "dist/gymnast.min.js",
  "homepage": "https://gymnastjs.github.io/gymnast",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "src",
    "dist",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gymnastjs/gymnast"
  },
  "bugs": {
    "url": "https://github.com/gymnastjs/gymnast/issues"
  },
  "author": "Oscar Bartra",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-actions": "3.3.13",
    "@storybook/addon-knobs": "3.3.13",
    "@storybook/addon-notes": "3.3.13",
    "@storybook/addon-options": "3.3.13",
    "@storybook/channels": "3.3.13",
    "@storybook/react": "3.3.13",
    "all-contributors-cli": "5.4.0",
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.2",
    "babel-plugin-flow-react-proptypes": "24.1.1",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-macros": "2.4.0",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-preval": "3.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "6.26.0",
    "bundlesize": "0.17.0",
    "clean-webpack-plugin": "0.1.19",
    "codeclimate-test-reporter": "0.5.0",
    "condition-circle": "2.0.1",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "3.4.4",
    "enzyme-adapter-react-16": "1.2.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.4.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "extract-text-webpack-plugin": "3.0.2",
    "find-up": "3.0.0",
    "flow-bin": "0.79.1",
    "flow-copy-source": "2.0.2",
    "html-loader": "0.5.5",
    "ignore-loader": "0.1.2",
    "jest": "23.5.0",
    "lodash": "4.17.10",
    "markdown-loader": "3.0.0",
    "mime": "2.3.1",
    "nightwatch": "0.9.21",
    "npm-run-all": "4.1.3",
    "picturebook": "1.2.6",
    "polished": "1.9.3",
    "postcss-import": "12.0.0",
    "postcss-loader": "3.0.0",
    "postcss-nesting": "6.0.0",
    "prettier": "1.14.2",
    "raw-loader": "0.5.1",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-element-to-jsx-string": "14.0.1",
    "react-test-renderer": "16.4.2",
    "regenerator-runtime": "0.12.1",
    "saucelabs": "1.5.0",
    "selenium-standalone": "6.15.1",
    "semantic-release": "15.9.9",
    "storybook-addon-jsx": "5.4.0",
    "webpack": "3.11.0",
    "webpack-bundle-analyzer": "2.13.1"
  },
  "bundlesize": [
    {
      "path": "./dist/gymnast.min.js",
      "maxSize": "11.0 Kb"
    }
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/{,**/}*.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/env.js",
    "moduleNameMapper": {
      "^.+[.](md|txt)$": "<rootDir>/test/stringStub.js"
    },
    "transform": {
      "^.+[.]js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules\\/(?!picturebook)"
    ]
  },
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development webpack --config scripts/webpack.config.js",
    "build:min": "cross-env NODE_ENV=production webpack --config scripts/webpack.config.js -p",
    "build:types": "sh scripts/flowsource.sh",
    "build": "npm-run-all --parallel build:*",
    "clean:image": "find storybook/stories -name \"*.png\" -type f -delete",
    "commit": "git-cz",
    "contributors:add": "all-contributors add",
    "contributors:gen": "all-contributors generate",
    "format": "prettier --write --single-quote --no-semi --trailing-comma es5 \"{src,test,storybook}/**/*.js\"",
    "lint:flow": "flow stop && flow --show-all-errors --include-warnings",
    "lint:js": "eslint scripts src storybook test",
    "lint": "npm-run-all --parallel lint:*",
    "start:storybook": "picturebook -p 9001 -s ./static -c node_modules/picturebook/config",
    "start:webpack": "yarn build:dev --watch",
    "start": "npm-run-all --parallel start:*",
    "test:image": "picturebook-image --env iphone7,ie11,safari,firefox,edge,chrome --suiteRetries 3",
    "test:size": "bundlesize",
    "test": "jest",
    "test:cover": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "release": {
    "verifyConditions": "condition-circle"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "prop-types": ">=15",
    "react": ">=15",
    "react-dom": ">=15"
  },
  "dependencies": {
    "cxs": "https://github.com/arahansen/cxs#d593d14e708c4f5c17cb2105cb9435ae87ade362",
    "preval.macro": "3.0.0"
  }
}
