{
  "name": "gymnast",
  "version": "0.0.0-semantically-released",
  "description": "Configurable grid and layout engine for React",
  "main": "index.js",
  "unpkg": "dist/gymnast.min.js",
  "homepage": "https://gymnastjs.github.io/gymnast",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "src",
    "dist",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gymnastjs/gymnast"
  },
  "bugs": {
    "url": "https://github.com/gymnastjs/gymnast/issues"
  },
  "author": "Oscar Bartra",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.1.6",
    "@babel/preset-flow": "7.0.0",
    "@storybook/addon-actions": "4.0.8",
    "@storybook/addon-knobs": "4.0.8",
    "@storybook/addon-notes": "4.0.8",
    "@storybook/addon-options": "4.0.8",
    "@storybook/addons": "4.0.8",
    "@storybook/react": "4.0.8",
    "all-contributors-cli": "5.4.1",
    "babel-cli": "6.26.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-macros": "2.4.2",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-preval": "3.0.1",
    "babel-runtime": "6.26.0",
    "bundlesize": "0.17.0",
    "clean-webpack-plugin": "1.0.0",
    "codeclimate-test-reporter": "0.5.1",
    "condition-circle": "2.0.1",
    "cross-env": "5.2.0",
    "css-loader": "1.0.1",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.9.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "extract-text-webpack-plugin": "3.0.2",
    "find-up": "3.0.0",
    "flow-bin": "0.86.0",
    "flow-copy-source": "2.0.2",
    "flow-typed": "2.5.1",
    "html-loader": "0.5.5",
    "ignore-loader": "0.1.2",
    "jest": "23.6.0",
    "lodash": "4.17.11",
    "markdown-loader": "4.0.0",
    "nightwatch": "0.9.21",
    "npm-run-all": "4.1.3",
    "picturebook": "2.0.0-beta.18",
    "polished": "2.3.0",
    "postcss-cssnext": "3.1.0",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-nesting": "7.0.0",
    "prettier": "1.15.2",
    "raw-loader": "0.5.1",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-element-to-jsx-string": "14.0.2",
    "react-test-renderer": "16.6.3",
    "regenerator-runtime": "0.13.1",
    "require-context": "1.1.0",
    "semantic-release": "15.12.0",
    "storybook-addon-jsx": "5.4.0",
    "style-loader": "0.23.1",
    "stylelint": "9.8.0",
    "webpack": "4.26.0",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2"
  },
  "bundlesize": [
    {
      "path": "./dist/gymnast.min.js",
      "maxSize": "11.0 Kb"
    }
  ],
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development webpack --config scripts/webpack.config.js --mode=development",
    "build:min": "cross-env NODE_ENV=production webpack --config scripts/webpack.config.js --mode=production",
    "build:types": "sh scripts/flowsource.sh",
    "build": "npm-run-all --parallel build:*",
    "clean:image": "find storybook/stories -name \"*.png\" -type f -delete && find storybook/stories -name __screenshots__ -type d -exec rm -rf {} \\;",
    "contributors:add": "all-contributors add",
    "contributors:gen": "all-contributors generate",
    "format": "prettier --write --single-quote --no-semi --trailing-comma es5 \"{src,test,storybook}/**/*.js\"",
    "lint:css": "stylelint stories/*.css",
    "lint:flow": "flow stop && flow --show-all-errors --include-warnings",
    "lint:js": "eslint scripts src storybook test",
    "lint": "npm-run-all --parallel lint:*",
    "start:storybook": "start-storybook -p 9001 -s ./static -c storybook",
    "start:webpack": "yarn build:dev --watch",
    "start": "npm-run-all --parallel start:*",
    "test:image": "LOCAL_BRANCH=$(git rev-parse --abbrev-ref HEAD) node test/img.js --env iphone7,chrome,firefox,ie11,edge,safari",
    "test:size": "bundlesize",
    "test": "jest",
    "test:cover": "jest --coverage --config jest.config.js",
    "test:watch": "jest --watch"
  },
  "release": {
    "verifyConditions": "condition-circle"
  },
  "peerDependencies": {
    "react": ">=16.3",
    "react-dom": ">=15"
  },
  "dependencies": {
    "cxs": "https://github.com/arahansen/cxs#d593d14e708c4f5c17cb2105cb9435ae87ade362",
    "preval.macro": "3.0.0"
  }
}
