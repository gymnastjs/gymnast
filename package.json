{
  "name": "xn-reflex",
  "version": "0.0.0-semantically-released",
  "description": "Flexbox React 12-column layout system",
  "module": "src/index.js",
  "main": "dist/reflex.js",
  "browser": "dist/reflex.js",
  "homepage": "https://obartra.github.io/reflex",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "src",
    "dist",
    "version.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/obartra/reflex"
  },
  "bugs": {
    "url": "https://github.com/obartra/reflex/issues"
  },
  "author": "Oscar Bartra",
  "license": "MIT",
  "devDependencies": {
    "@bubltechnology/customizable-commit-analyzer": "1.0.2-0",
    "@storybook/addon-knobs": "3.1.6",
    "@storybook/addon-notes": "3.1.6",
    "@storybook/addon-storyshots": "3.1.6",
    "@storybook/react": "3.1.6",
    "all-contributors-cli": "4.3.0",
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-plugin-flow-react-proptypes": "3.4.2",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-runtime": "6.23.0",
    "bundlesize": "^0.12.0",
    "clean-webpack-plugin": "0.1.16",
    "codeclimate-test-reporter": "0.5.0",
    "commitizen": "2.9.6",
    "condition-circle": "1.5.0",
    "cross-env": "5.0.1",
    "css-loader": "0.28.4",
    "cz-conventional-changelog": "2.0.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.2",
    "eslint-config-prettier": "2.3.0",
    "eslint-plugin-flowtype": "2.34.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "flow-bin": "0.49.1",
    "html-loader": "0.4.5",
    "husky": "0.14.3",
    "identity-obj-proxy": "3.0.0",
    "ignore-loader": "0.1.2",
    "jest": "20.0.4",
    "lint-staged": "4.0.1",
    "markdown-loader": "2.0.1",
    "nightwatch": "0.9.16",
    "node-resemble-js": "0.2.0",
    "npm-run-all": "4.0.2",
    "postcss-cli": "4.1.0",
    "postcss-cssnext": "3.0.0",
    "postcss-discard-comments": "2.0.4",
    "postcss-for": "2.1.1",
    "postcss-import": "10.0.0",
    "postcss-loader": "2.0.6",
    "postcss-nesting": "4.0.1",
    "prettier": "1.5.2",
    "raw-loader": "0.5.1",
    "react-dom": "15.6.1",
    "react-element-to-jsx-string": "10.1.0",
    "react-test-renderer": "15.6.1",
    "regenerator-runtime": "0.10.5",
    "selenium-standalone": "6.5.0",
    "semantic-release": "6.3.6",
    "semantic-release-cli": "3.0.3",
    "storybook-addon-jsx": "4.1.1",
    "style-loader": "0.18.2",
    "stylelint": "7.12.0",
    "webpack": "3.1.0",
    "webpack-bundle-analyzer": "2.8.2"
  },
  "lint-staged": {
    "*.{js,css}": [
      "format",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^.+[.]css$": "identity-obj-proxy",
      "^.+[.](md|txt)$": "<rootDir>/test/stringStub.js"
    },
    "testMatch": [
      "**/?(*.)spec.js"
    ]
  },
  "bundlesize": [
    {
      "path": "./dist/reflex.min.js",
      "maxSize": "6 Kb"
    },
    {
      "path": "./dist/reflex.css",
      "maxSize": "1.5 Kb"
    }
  ],
  "scripts": {
    "build:dev": "webpack --config scripts/webpack.config.js",
    "build:min": "cross-env NODE_ENV=production webpack --config scripts/webpack.config.js -p",
    "build": "npm-run-all --parallel build:*",
    "commit": "git-cz",
    "contributors:add": "all-contributors add",
    "contributors:gen": "all-contributors generate",
    "format-all": "git ls-files {,**/**/}*.{js,css} | xargs yarn format",
    "format": "prettier --write --single-quote --no-semi --trailing-comma es5",
    "lint:css": "stylelint src/*.css stories/*.css",
    "lint:flow": "flow stop && flow",
    "lint:js": "eslint src stories storybook",
    "lint": "npm-run-all --parallel lint:*",
    "precommit": "lint-staged",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "start:storybook": "start-storybook -p 9001 -s ./storybook/static -c storybook/config",
    "start:webpack": "yarn build:dev -- --watch",
    "start": "npm-run-all --parallel start:*",
    "test:image": "babel-node node_modules/.bin/nightwatch --config test/screenshot/nightwatch.conf.js",
    "test:size": "bundlesize",
    "test": "jest"
  },
  "release": {
    "verifyConditions": "condition-circle",
    "analyzeCommits": "@bubltechnology/customizable-commit-analyzer"
  },
  "dependencies": {
    "lodash": "4.17.4",
    "prop-types": "15.5.10",
    "react": "15.6.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
