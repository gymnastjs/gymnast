{
  "presets": [
    "es2015",
    "react"
  ],
  "plugins": [
    "flow-react-proptypes",
    "lodash",
    "transform-object-rest-spread",
    "transform-class-properties"
  ],
  "env": {
    "production": {
      "plugins": [
        [
          "module-resolver",
          {
            "alias": {
              "reflex": "./dist/reflex"
            }
          }
        ]
      ]
    },
    "development": {
      "plugins": [
        [
          "module-resolver",
          {
            "alias": {
              "reflex": "./dist/reflex"
            }
          }
        ]
      ]
    },
    "test": {
      "plugins": [
        [
          "module-resolver",
          {
            "alias": {
              "reflex": "./src/index"
            }
          }
        ]
      ]
    }
  }
}
