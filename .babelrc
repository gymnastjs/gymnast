{
  "presets": [
    "es2015",
    "react"
  ],
  "plugins": [
    "flow-react-proptypes",
    "lodash",
    "transform-object-rest-spread",
    "transform-class-properties"
  ],
  "env": {
    "production": {
      "plugins": [
        [
          "module-resolver",
          [
            {
              "src": "./dist/reflex",
              "expose": "reflex"
            }
          ]
        ]
      ]
    },
    "development": {
      "plugins": [
        [
          "module-resolver",
          [
            {
              "src": "./dist/reflex",
              "expose": "reflex"
            }
          ]
        ]
      ]
    },
    "test": {
      "plugins": [
        [
          "module-resolver",
          [
            {
              "src": "./src/index",
              "expose": "reflex"
            }
          ]
        ]
      ]
    }
  }
}
