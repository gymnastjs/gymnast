[ignore]
.*/node_modules/conventional-changelog-core/test
.*/node_modules/editions/.*
.*/node_modules/radium/.*
.*/node_modules/stylelint/.*
.*/node_modules/eslint-plugin-jsx-a11y/.*
.*/node_modules/config-chain/test/broken.json
.*/node_modules/npm/node_modules/config-chain/test/broken.json
.*/node_modules/npmconf/test/fixtures/package.json
.*/node_modules/react-element-to-jsx-string/.*

[include]

[libs]

[options]
module.name_mapper='reflex' -> '<PROJECT_ROOT>/src/reflex.js'
module.name_mapper='.*\(.css\)' -> 'CSSModule'
module.name_mapper.extension='txt' -> '<PROJECT_ROOT>/test/stringStub.js'
module.name_mapper.extension='md' -> '<PROJECT_ROOT>/test/stringStub.js'
module.ignore_non_literal_requires=true
suppress_comment= \\(.\\|\n\\)*\\$FlowDisable
